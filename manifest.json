{
  "manifest_version": 3,
  "name":"ufocus",
  "description": "udemy focus mode",
  "version": "1.0",
  "permissions": ["tabs", "nativeMessaging"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
      "https://www.udemy.com/course/*"
    ],
    "js": ["content_script.js"]
  }]
}

